{
    "name": "sitecrafting/wp-cli-scaffold-groot-command",
    "description": "",
    "type": "wp-cli-package",
    "homepage": "https://github.com/sitecrafting/wp-cli-scaffold-groot-command",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "GrootScaffold\\": "src/GrootScaffold/"
        }
    },
    "autoload-dev": {
        "files": [
            "test/bootstrap.php"
        ],
        "psr-4": {
            "GrootScaffoldTest\\": "test/cases/"
        }
    },
    "require": {
        "wp-cli/wp-cli": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "wp-cli/core-command": "~2.0"
    },
    "scripts": {
        "post-install-cmd": "if [ ! -d wp ] ; then ./vendor/bin/wp core download --path=wp; fi"
    }
}
